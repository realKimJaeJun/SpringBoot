<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.user.dao.User6DAO">

	<insert id="insertUser6">
		INSERT INTO `user6` VALUES (#{uid}, #{name}, #{birth}, #{age}, #{address}, #{hp});
	</insert>
	
	<select id="selectUser6" resultType="kr.co.user.vo.User6VO">
	select * from `user6` where `uid` = #{uid};
	</select>
		
	<select id="selectUser6s" resultType="kr.co.user.vo.User6VO">
		select * from `user6`;
	</select>
	
	<update id="updateUser6">
		update `user6` set
				`name`=#{name},
				`birth`=#{birth},
				`age`=#{age},
				`address`=#{address},
				`hp`=#{hp}
		where 
				`uid`=#{uid};
	</update>
	<delete id="deleteUser6">
		delete from `user6` where `uid`=#{uid};
	</delete>
</mapper>